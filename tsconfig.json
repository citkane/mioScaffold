{
	"include": [
		"./src/**/*.d.ts",
		"./src/**/*"
	],
	"typedocOptions": {
		"entryPoints": [
			"./src/mioScaffold.ts",
		],
		"plugin": [],
		"entryPointStrategy": "resolve",
		"externalPattern": "**/node_modules/**",
		"excludeExternals": true,
		"out": "docs",
		"readme": "none"
	},
	"compilerOptions": {
		"outDir": "./build",
		"target": "es2017", 
		"module": "CommonJS",
		"moduleResolution": "Node",
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}